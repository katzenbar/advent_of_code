{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext", "es2019"],
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true
  }
}
